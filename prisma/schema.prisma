generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int    @id @unique
  username      String
  commandPrefix String @map("command_prefix")
  scriptPrefix  String @map("script_prefix")
  adminPrefix   String @map("admin_prefix")
}
