generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int    @id @unique
  rank          Int    @default(1)
  token         String @db.Text()
  username      String @default("UserName")
  squad         String @default("hexvel")
  cover_image   String @default("photo-224389197_457239529")
  commandPrefix String @default(".н") @map("command_prefix")
  scriptPrefix  String @default(".нс") @map("script_prefix")
  adminPrefix   String @default(".на") @map("admin_prefix")
  list          Json   @default("{\"ignore\": [], \"trust\": []}")
}
